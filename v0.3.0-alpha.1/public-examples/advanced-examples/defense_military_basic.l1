// Defense: Military - Basic Calculations
println("=== DEFENSE: Military Calculator ===");

function calculate_range(velocity, angle) {
    let g = 9.81;
    let range = (velocity * velocity * 0.866) / g;
    return range;
}

function calculate_blast_radius(mass) {
    let radius = 10.0 * mass;
    return radius;
}

// Test scenarios
